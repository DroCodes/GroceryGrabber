@{
    Layout = "_Layout";
}

<div>
    <h2 class="text-center">Select a list to edit</h2>
    <table class="table table-bordered">
        <tbody>
            @for (int i = 0; i < ViewBag.length; i++)
            {
                <tr>
                    <td><input type="checkbox" /></td>
                    <td>@ViewBag.listName[i]</td>
                </tr>
            }
        </tbody>
    </table>
    <button class="btn btn-danger" asp-controller="List" asp-action="">Delete</button>
</div>
